    </div>
    <!-- SF RVPs -->
    <div id="sfrvpRoot" class="SFRVPwrapper">
        <div id="sfrvpHandle" class="SFRVPheader headerCommon">SF RVPs</div>
        <div class="SFRVPlist ">
        <apex:outputPanel id="SFRVPlist">
